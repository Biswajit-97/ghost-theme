<div class='js-grid__col o-grid__col o-grid__col--4-4-s o-grid__col--2-4-m o-grid__col--2-4-l js-post-card-wrap c-post-card-wrap '>
  <div class='c-post-card  c-post-card--featured  c-post-card--with-image  c-post-card--no-image '>
    
      <div class='c-post-card__media'>
        <a href='{{ url }}' class='c-post-card__image-wrap' >
          <img
            class='c-post-card__image lazyload'
            data-src='{{ img_url feature_image size='l' }}'
          />
        </a>
      </div>
 

    <div class='c-post-card__content  c-post-card__content--no-image '>
      <div class='c-post-card__meta'>
        
          <a href='{{ url }}' class='c-post-card__author'>{{ name }}</a>
        

        <time class='c-post-card__date' datetime='{{ date format='YYYY-MM-DD' }}' title='{{ date format='DD MMMM YYYY' }}'>{{ date format='DD.MMM.YYYY' }}</time>

      </div>

      <h2 class='c-post-card__title'>
        <a href='{{ url }}' class='c-post-card__title-link'>
          {{ title }}
        </a>
      </h2>

      <a href='{{ url }}' class='c-post-card__excerpt'>{{ excerpt }}</a>
    </div>
  </div>
</div>